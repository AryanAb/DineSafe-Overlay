{
  "name": "Health Inspector - BETA",
  "description": "Check health inspection records for restaurants and bars - THIS EXTENSION IS FOR BETA TESTING",
  "version": "0.1",
  "permissions": ["scripting", "storage"],
  "host_permissions": ["https://www.google.ca/maps/*", "https://secure.toronto.ca/opendata/ds/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.ca/maps/*"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}